{
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "eqeqeq": "error",
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "curly": ["error", "all"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react-hooks/exhaustive-deps": "warn"
  }
}
